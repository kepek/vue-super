{
  "name": "vue-super",
  "version": "0.0.2",
  "description": "Vue.js plugin that allows you to reference methods on parent classes",
  "main": "vue-super.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpkilby/vue-super.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "super",
    "inheritance"
  ],
  "author": "Ryan P Kilby <rpkilby@ncsu.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rpkilby/vue-super/issues"
  },
  "homepage": "https://github.com/rpkilby/vue-super#readme",
  "jspm": {
    "format": "esm"
  },
  "eslintConfig": {
    "extends": [
      "defaults/configurations/eslint",
      "defaults/configurations/airbnb"
    ],
    "env": {
      "es6": true,
      "mocha": true,
      "node": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "rules": {
      "indent": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "func-names": 0,
      "curly": [
        2,
        "multi-or-nest"
      ],
      "no-reserved-keys": 0,
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "varsIgnorePattern": "__"
        }
      ]
    }
  }
}
